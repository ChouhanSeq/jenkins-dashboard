{
  "name": "jenkins",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "server": "nodemon server.js --quiet",
    "dev": "yarn server | rsbuild dev",
    "build": "rsbuild build",
    "start": "yarn build && NODE_ENV=production pm2 start server.js --name jenkins && pm2 save",
    "stop": "pm2 delete jenkins && pm2 save",
    "restart": "yarn stop && yarn start",
    "logs": "pm2 logs"
  },
  "dependencies": {
    "@million/lint": "^0.0.73",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "million": "^3.0.6",
    "nodemon": "^3.1.0",
    "pm2": "^5.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "shrink-ray-current": "^4.1.3"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.5.6",
    "@rsbuild/plugin-react": "^0.5.6"
  }
}
